{
  "name": "the-way-final-page",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "analyse": "yarn lint && yarn build-stats && webpack-bundle-analyzer dist/webapp/stats-es5.json",
    "build-stats": "ng build --stats-json",
    "clean": "node ../build-tools/main.js --type=interface --path=src/app/core/security --systemToken=1",
    "lint": "ng lint && stylelint \"./**/*.scss\" ",
    "publish-page": "ng build --prod --base-href https://nihasoft.github.io/the-way/ && git checkout gh-pages-final && yarn install && copyfiles ./dist .",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "test": "yarn lint && ng test --browsers CustomChromeHeadless",
    "test-coverage": "yarn lint && ng test --code-coverage --browsers CustomChromeHeadless --no-watch"
  },
  "author": "Hanor SÃ¡tiro Cintra",
  "private": true,
  "dependencies": {
